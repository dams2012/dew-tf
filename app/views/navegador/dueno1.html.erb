<div class="row">

<div class="col"></div>
    
    <div class="col">

      <h1>Registrar dueño de estacionamiento</h1>

      <!--<form method="post" action='/login'>-->
      <form>

      <div class="form-group">
        <label for="names">Nombres</label>
        <input type="text" class="form-control" id="names" name="names" aria-describedby="emailHelp" placeholder="Ingrese sus nombres">
      </div>

      <div class="form-group">
        <label for="lastname1">Apellido Paterno</label>
        <input type="text" class="form-control" id="lastname1" name="lastname1" aria-describedby="emailHelp" placeholder="Ingrese su apellido paterno">
      </div>

      <div class="form-group">
        <label for="lastname2">Apellido Materno</label>
        <input type="text" class="form-control" id="lastname2" name="lastname2" aria-describedby="emailHelp" placeholder="Ingrese su apellido materno">
      </div>

      <div class="form-group">
        <label for="gender">Sexo</label>
          <select class="form-control" id="gender">
            <option>Masculino</option>
            <option>Femenino</option>
          </select>
      </div>

      <div class="form-group">
        <label for="document">Tipo de documento de identidad</label>
          <select class="form-control" id="document">
            <option>DNI o Libreta Militar</option>
            <option>Carnet de extranjería</option>
            <option>Pasaporte</option>
          </select>
      </div>


      <div class="form-group">
        <label for="num_doc">Número de documento</label>
        <input type="number" class="form-control" id="num_doc" name="num_doc" aria-describedby="emailHelp" placeholder="Ingrese su número de documento">
      </div>


      <div class="form-group">
        <label for="type_user">Tipo de usuario</label>
          <select class="form-control" id="type_user">
            <option>Dueño de estacionamiento</option>
            <option>Cliente</option>
          </select>
      </div>


      <div class="form-group">
        <label for="email">Correo electrónico</label>
        <input type="email" class="form-control" id="email" name="email" aria-describedby="emailHelp" placeholder="Ingrese correo electrónico">
      </div>

      <div class="form-group">
        <label for="confirm_email">Confirme correo electrónico</label>
        <input type="email" class="form-control" id="confirm_email" name="confirm_email" aria-describedby="emailHelp" placeholder="Confirme correo electrónico">
      </div>
 
     <div class="form-group">
        <label for="password">Contraseña</label>
        <input type="password" class="form-control" id="password" name="password" placeholder="Ingrese contraseña">
     </div>

     <div class="form-group">
        <label for="confirm_password">Confirme contraseña</label>
        <input type="password" class="form-control" id="confirm_password" name="confirm_password" placeholder="Confirme contraseña">
     </div>

     <div class="form-group">
        <label for="birthday">Fecha de nacimiento</label>
        <input type="date" class="form-control" id="birthday" name="birthday" placeholder="Ingrese fecha de nacimiento">
     </div>

     <div class="form-group">
        <label for="cell_number">Número de celular</label>
        <input type="tel" class="form-control" id="cell_number" name="cell_number" placeholder="Ingrese número de celular">
     </div>
      
   
      <button type="button" class="btn btn-dark btn-block"> <a href="/navegador/dueno2" style="color:inherit"> Regístrate </a> </button>
  </form>

   </div>
    
    <div class="col"></div>

</div>
